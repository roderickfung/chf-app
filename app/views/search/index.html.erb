<h1>Search</h1>
<table class='table-striped table' style='margin-top: 5 rem;'>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>E-Mail</th>
    <th>View Details</th>
  </tr>
  <% @pgsearch.each do |x|%>
    <tr>
      <td><%=x.first_name%></td>
      <td><%=x.last_name%></td>
      <td><%=x.email%></td>
      <td>
        <%=link_to search_path(x) do%>
          <i class='material-icons'>info</i>
        <%end%>
      </td>
    </tr>
  <%end%>
</table>
